function aAttack()
    spawn(function()
        while getgenv().attackplayer == true do 
                for i,v in next, game:GetService("Players"):GetPlayers() do
                        if v ~= game:GetService("Players").LocalPlayer then
                        local args = {[1] =game:GetService("Players")[v.Name]}game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CursorCannonService.FireCursor:FireServer(unpack(args))
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(546.973083, 28.8025417, -173.112762, 0.461820334, 0, -0.886972487, 0, 1, 0, 0.886972427, 0, 0.4618201)  
                    wait()
                end
            end
        end
    end)
end
function autoRebirth(RebirthAmount)
    spawn(function()
        while getgenv().autoRebirth == true do
            remotePath.RebirthService.BuyRebirths:FireServer(RebirthAmount)
            wait()
        end
    end)
 end

 function autoCollectDrops()
    spawn(function()
        while wait() do
            if not getgenv().Pickup then break end
            for i,v in pairs(game:GetDescendants()) do
                if v.Name=="TouchInterest" and v.Parent.Parent.Name=="BasePickup" then
                    local plyrHead=game.Players.LocalPlayer.Character.HumanoidRootPart
                    firetouchinterest(plyrHead,v.Parent,0)
                end
            end
        end
    end)
  end
  function autoEggs(EggAmount)
    spawn(function()  
            while getgenv().autoEgg == true do
            remotePath.EggService.Purchase:FireServer(EggAmount)
            wait()
        end
    end)
end
function bAttack()
    spawn(function()
        while getgenv().bossAttack == true do
            local args = {[1] = "Easter Bunny"}game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CursorCannonService.FireBoss:FireServer(unpack(args))
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(789.0367431640625, 8.732751846313477, -268.4192199707031)  
            wait()
        end
    end)
end
function sRebirth(SRebirthAmount)
    spawn(function()
             while getgenv().autoSuperRebirth == true do
            remotePath.RebirthService.BuySuperRebirths:FireServer(SRebirthAmount)
            wait()
        end
    end)
end
function aUpgrade(UpgradeType)
    spawn(function()
        while getgenv().autoUpgrade == true do
            remotePath.UpgradeService.BuyUpgrade:FireServer(UpgradeType)
            wait()
        end
    end)
end
function sUpgrade(SUpgradeType)
    spawn(function()
        while getgenv().autoSuperUpgrade == true do
            remotePath.SuperUpgradeService.BuyUpgrade:FireServer(SUpgradeType)
            wait()
        end
    end)
end



--GUI
local Tabs = {
    -- Creates a new tab titled Main
    Main = Window:AddTab('Main'),
}

local LeftGroupBox = Tabs.Main:AddLeftGroupbox('Main Features')

LeftGroupBox:AddToggle('AutoClick', {
    Text = 'Auto Click Normal',
    Default = false, -- Default value (true / false)
    Tooltip = 'Auto Clicks At Normal Speeds', -- Information shown when you hover over the toggle
})

Toggles.AutoClick:OnChanged(function(bool)
    getgenv().autoTap = bool
        print('Auto Click Normal On', bool);
        if bool then
        aTap()
	end  
end)

